SET	E0	00FF
SET	E1	FF00
SET	FD	0000	;SHOULD ALWAYS BE ZERO
SET	FE	0001	;USED TO INCREMENT TIMER/COMPARISON
SET	FF	FFFF	;DELAY TIMER

LOOP:
BRA	SWAP
SUBLOOP:
LOAD	G0	DELAY_BEQ
COMB	G0	$LOOP
STORE	G0	DELAY_BEQ
BRA	DELAY


SWAP:
LOAD	G0	E0
LOAD	G1	E1
STORE	G0	E1
STORE	G1	E0
BRA	SUBLOOP

DELAY:
LOAD	G0	FD
DELAY_SUB:
ADD	G0	FE
CMP	G0	FF
DELAY_BEQ:
BEQ	00
BRA	DELAY_SUB



